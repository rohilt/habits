{
  "name": "svelte-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public --no-clear --single",
    "validate": "svelte-check",
    "lint": "prettier --write --plugin-search-dir=. ./src/**/*",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.14.0",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@rollup/plugin-typescript": "^8.0.0",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/svelte": "^3.0.3",
    "@tsconfig/svelte": "^1.0.0",
    "@types/jest": "^26.0.23",
    "@wasm-tool/rollup-plugin-rust": "^1.0.6",
    "babel-jest": "^26.6.3",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "prettier-plugin-svelte": "^2.2.0",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.0.0",
    "svelte-check": "^1.0.0",
    "svelte-jester": "^1.5.0",
    "svelte-preprocess": "^4.0.0",
    "ts-jest": "^26.5.5",
    "tslib": "^2.0.0",
    "typescript": "^4.0.0"
  },
  "dependencies": {
    "autoprefixer": "^10.2.5",
    "chart.js": "^3.2.1",
    "page": "^1.11.6",
    "postcss": "^8.2.13",
    "sirv-cli": "^1.0.0",
    "svelte-file-dropzone": "^0.0.15",
    "tailwindcss": "^2.1.2"
  },
  "jest": {
    "transform": {
      "^.+\\.svelte$": [
        "svelte-jester",
        {
          "preprocess": true
        }
      ],
      "^.+\\.ts$": "ts-jest",
      "^.+\\.js$": "babel-jest"
    },
    "moduleFileExtensions": [
      "js",
      "ts",
      "svelte"
    ]
  }
}
